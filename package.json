{
  "name": "awesomelist",
  "version": "1.0.0",
  "main": "src/App.tsx",
  "scripts": {
    "start": "expo start",
    "start:dev": "expo start --dev-client",
    "android:virtual": "eas build:run -p android",
    "android": "expo run:android",
    "android:eas:dev": "eas build --platform android --profile development",
    "android:eas:pre": "eas build --platform android --profile preview",
    "prebuild:clean": "npx expo prebuild --clean",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "prettier:write": "npx prettier --write **/*.{js,jsx,ts,tsx,json} && npx prettier --write *.{js,jsx,ts,tsx,json}",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx ./",
    "lint:fix": "npx eslint ./src/**/*.tsx --fix",
    "format": "prettier --check ./src",
    "format:fix": "npx prettier --write ./src"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-firebase/app": "^19.0.1",
    "@react-native-firebase/auth": "^19.0.1",
    "@react-native-firebase/firestore": "^19.0.1",
    "@react-native-firebase/functions": "^19.0.1",
    "@react-native-google-signin/google-signin": "^11.0.0",
    "@react-navigation/native": "^6.1.6",
    "@react-navigation/native-stack": "^6.9.12",
    "axios": "^1.7.2",
    "expo": "~52.0.7",
    "expo-checkbox": "~4.0.0",
    "expo-dev-client": "~5.0.1",
    "expo-status-bar": "~2.0.0",
    "firebase": "^11.0.2",
    "react": "18.3.1",
    "react-native": "0.76.2",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~18.3.12",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-native": "^4.0.0",
    "prettier": "^2.8.8",
    "typescript": "^5.3.0"
  },
  "private": true,
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "listUnknownPackages": false
      }
    }
  }
}
